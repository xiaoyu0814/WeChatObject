/* pages/afterLive/aftreLive.wxss */
.afterLive_bg {
  position: fixed;
  width: 100%;
  height: 100%;
  background-size: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  flex-direction: column;
}

.liveTime {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-top: 1px solid #fff5;
  border-bottom: 1px solid #fff5;
  height: 100px;
  margin: 50px;
  width: 250px;
}

.back{
  color: #fff;
  background-color: rgb(0, 122, 255);
  width: 120px;
  height: 50px;
  border-radius: 500px;
}